
#!/usr/bin/env perl

use strict;
use warnings;
use 5.014;
use File::Copy;

die "Filename not found!\n" if @ARGV == 0;

foreach my $filename(@ARGV){
	die "File $filename already exist!\n" if -e $filename;
	copy(DATA, "$filename") or die $!;
	chmod 0777, $filename;
} 
__DATA__
#!/usr/bin/env perl
use 5.014;
use strict;
use warnings;
use utf8;
use open qw/:encoding(UTF-8) :std/;



